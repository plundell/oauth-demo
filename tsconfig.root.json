{
	"compilerOptions": {
		"target": "ES2022",
		"module": "esnext",
		"moduleResolution": "bundler",
		"strict": true,
		"jsx": "preserve",
		"baseUrl": ".",
		"paths": {},
		"lib": [
			"dom",
			"dom.iterable",
			"esnext"
		],
		"skipLibCheck": true,
		"allowJs": true,
		"noEmit": true
	},
	"include": [
		"**/*.ts",
		"**/*.tsx"
	],
	"exclude": [
		"node_modules"
	]
}